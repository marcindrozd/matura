<%= form_for @group do |f| %>
  <%= f.label :name, 'Nazwa grupy' %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :group_type, 'Typ' %>
  <%= f.select :group_type do %>
    <%= options_for_select([['dwujęzyczna'], ['rozszerzona']]) %>
  <% end %>
  <br />
  <%= f.submit value='Utwórz grupę' %>
<% end %>
