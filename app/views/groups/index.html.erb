<h1>List of groups</h1>

<table>
  <tr>
    <th>Nazwa grupy</th>
    <th>Typ grupy</th>
  </tr>
  <% @groups.each do |group| %>
    <tr>
      <td><%= group.name %></td>
      <td><%= group.group_type %></td>
      <td>
        <%= link_to 'Wyświetl', group %><br />
        <%= link_to 'Edytuj', edit_group_path(group) %>
      </td>
    </tr>
  <% end %>
</table>

<%= link_to 'Utwórz nową grupę', new_group_path %>
